{
  "name": "klapuch/uri",
  "type": "library",
  "description": "Normalized, united, checked and beauty uri",
  "homepage": "https://github.com/klapuch/Uri",
  "keywords": [
    "url",
    "urn",
    "uri"
  ],
  "authors": [
    {
      "name": "Dominik Klapuch",
      "email": "klapuchdominik@gmail.com",
      "homepage": "https://www.github.com/klapuch",
